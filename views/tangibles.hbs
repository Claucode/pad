{{#contentFor 'pageStyles'}}
<link rel="stylesheet" href="/stylesheets/pad.css" />
{{/contentFor}}

{{#contentFor 'pageScripts'}}
<script src="/javascripts/blocks.js"></script>
{{/contentFor}}

<div class="container">
  <ul class="tangibles">
    {{#each tangibles}}
      <li class="box tangible-box" rel="{{content.area}}">
        <a href="/tangible/{{uid}}" title="{{content.title}}">
          <span class="cover">{{content.title}}</span>
          <img src="/package/{{uid}}/image/front" alt="{{content.title}}">
        </a>
      </li>
    {{/each}}
  </ul>
  {{#if isEmpty}}
    <p class="ups-msg">Ups! No hay tangibles por el momento.</p>
  {{/if}}
</div>