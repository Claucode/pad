{{#contentFor 'pageStyles'}}
<link rel="stylesheet" href="/stylesheets/pad.css" />
<link rel="stylesheet" href="/stylesheets/dock-left.css" />
<link rel="stylesheet" href="/stylesheets/vendor/boots_gal/bootstrap-image-gallery.css" />
<link rel="stylesheet" href="/stylesheets/vendor/boots_gal/blueimp-gallery.min.css">
{{/contentFor}}

{{#contentFor 'pageScripts'}}
<script src="/javascripts/blocks.js"></script>
<script src='/javascripts/jquery.jqdock.min.js'></script>
<script src='/javascripts/clicks.js'></script>
<script src='/javascripts/jqdoc.anim.js'></script>
<script type='text/javascript' src='/javascripts/vendor/boots_gal/jquery.blueimp-gallery.min.js'></script>
<script type='text/javascript' src='/javascripts/vendor/boots_gal/bienvenida.js'></script>

  <div id="blueimp-gallery" class="blueimp-gallery">
  <div class="slides"></div>
    <h3 class="title"></h3>
    <a class="prev">‹</a>
    <a class="next">›</a>
    <a class="close">×</a>
    <a class="play-pause"></a>
    <ol class="indicator"></ol>
    <!-- The modal dialog, which will be used to wrap the lightbox content -->
    <div class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" aria-hidden="true">&times;</button>
                    <h4 class="modal-title"></h4>
                </div>
                <div class="modal-body next"></div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default pull-left prev">
                        <i class="glyphicon glyphicon-chevron-left"></i>
                        Previous
                    </button>
                    <button type="button" class="btn btn-primary next">
                        Next
                        <i class="glyphicon glyphicon-chevron-right"></i>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>

{{/contentFor}}
  <div id='page'>
    <div id='menu'>
      <img src='../images/dock_left/inicio.png' title='Inicio' alt='' />
      <img src='../images/dock_left/bienvenida.png'  id="image-gallery-button" title='Bienvenida' alt='' />
      <img src='../images/dock_left/nubesemantica.png' title='Nube' alt='' />
      <img src='../images/dock_left/orientaciones.png' title='Orientaciones' alt='' />
      <img src='../images/dock_left/temastransversales.png' title='Transversales' alt='' />
      <img src='../images/dock_left/contacto.png' title='Contacto' alt='' />      
    </div>
  </div>  
  <ul class="ejes no-js">
    {{#each ejes}}
      <li class="baraja" data-level="{{level}}" data-target="{{alt}}" data-owner="{{owner}}">
        <a href="{{href}}"><img src="/images/{{img}}" alt="{{alt}}"></a>
        <a href="{{href}}"><h4>{{title}}</h4></a>
      </li>
    {{/each}}
  </ul>
  {{#if isEmpty}}
    <div class="on-contruction">
      <img src="/images/padnet-construction.png" alt="">
    </div>
    <p class="ups-msg">Ups! No hay ejes por el momento.</p>
  {{/if}}
</div>


